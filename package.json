{
  "name": "mesg-js",
  "version": "1.2.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf ./lib && tsc && ncp ./src/client/proto/ ./lib/client/proto/",
    "prepublish": "npm run test && npm run build",
    "test": "ts-node ./node_modules/tape/bin/tape 'src/**/*_test.ts'",
    "coverage": "rimraf ./istanbul && tsc --project tsconfig-istanbul.json && ncp ./src/client/proto/ ./istanbul/client/proto/ && istanbul cover -x 'istanbul/**/*_test.js' --report html ./node_modules/.bin/tape 'istanbul/**/*_test.js'",
    "dev": "nodemon -e ts src --exec 'npm run test'"
  },
  "author": "Anthony Estebe <anthony@mesg.tech>",
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "grpc": "^1.13.0",
    "js-yaml": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/sinon": "^5.0.1",
    "@types/tape": "^4.2.32",
    "istanbul": "^0.4.5",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.2",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.3",
    "tape": "^4.9.1",
    "ts-node": "^7.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^2.9.2"
  },
  "typings": "lib/index.d.ts",
  "typescript": {
    "definition": "lib/index.d.ts"
  }
}
